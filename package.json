{
  "name": "uniswap-blocklet",
  "version": "0.0.3",
  "dependencies": {
    "axios": "^0.20.0"
  },
  "scripts": {
    "lint": "eslint src",
    "clean": "rm -rf public api/dist",
    "build:api": "NODE_ENV=production netlify-lambda build api/functions",
    "build:gatsby": "make prepare && node tools/create_env.js && GATSBY_APP_DOMAIN=www.arcblock.io gatsby build",
    "build": "npm run clean && npm-run-all build:*",
    "start:gatsby": "node tools/create_env.js && gatsby develop --host 0.0.0.0",
    "start:api": "nodemon api/index.js -w api"
  },
  "publishConfig": {
    "access": "public",
    "directory": "_blocklet/"
  },
  "files": [
    "build",
    "screenshots",
    "logo.png",
    "blocklet.md",
    "blocklet.js",
    "blocklet.json",
    "README.md"
  ],
  "blocklet": {
    "group": "static",
    "color": "secondary",
    "main": "build",
    "provider": "arcblock",
    "admin_url": "",
    "config_url": "",
    "public_url": "/"
  },
  "devDependencies": {},
  "license": "Apache-2.0"
}
